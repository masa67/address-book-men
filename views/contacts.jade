block content
  div(class='container')
    h1 Contacts
    div(class='username') User: #{username}
    ul(data-role='listview')
      each contact in contacts
        li
          a(onclick="abook.clickContactShow('#{contact._id}')") #{contact.name}
    button(onclick='abook.clickAddContact()') Add New Contact
    button(onclick='abook.clickLogout()') Logout